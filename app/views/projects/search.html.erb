<% @projects.each do |project| %>
<%= link_to "edit", edit_project_path(project), class: "btn btn-warning" %><%= link_to "delete", project_path(project), method: :delete, data: {confirm: "Are you sure? This is permanent and cannot be undone."}, class: "btn btn-danger" %> <%= link_to project.title, project_path(project) %><br />
<% end %>