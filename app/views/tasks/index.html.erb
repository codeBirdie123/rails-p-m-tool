<% @tasks.each do |task| %>
<%= link_to "edit", edit_task_path(task), class: "btn btn-warning" %><%= link_to "delete", task_path(task), method: :delete, class: "btn btn-danger" %><br />
<%= link_to task.title, task_path(task) %><br />
<%= task.description %><br />
<%= task.complete %><br />
<%= task.created_by %>
<hr>
<% end %>